    <script type="text/javascript">
       
         $('#cpn1').click(function () {
            if (this.checked) {
                $(".nb_cpn").show();
               // alert("Oui");
            }
        });
        $('#cpn2').click(function () {
            if (this.checked) {
                $("#nb_cpn").val('');
                $(".nb_cpn").hide();
                // alert("Non");
            }
        });
         $(".nb_cpn").hide();
          $(".bcf").click(function() {
