{% extends "themes/default/layout.html.twig" %}
{% block title %}
    Accouchement  {{ parent() }}
{% endblock %}

{% block content %}
    {% set pietable = '' %}

    <div class="wrapper">
        <br>
        <div class="row">
            {{ getMessages()|raw }}
        </div>
        <div class="widget">
            <div class="well row-fluid">
                <div  id="framework-content">
                    <div class="tabbable"><!-- default tabs -->
                        {% include "home/tab2.html.twig" %}
                        <div class="tab-content">
                            <div class="tab-pane active" id="tab1">                              
                                <div id="impactCreate">
                                    <input type="hidden" value="{{femme.id}}" class="span4" name="femme_id" id="femme_id"/>
                                    <div class="wrapper"> 
                                        <div class="wrapper_small"> 

                                            <div class="widget">
                                                <div class="tab-pane active" id="glyphs">
                                                    <ul class="clearfix">
                                                        <li><a href="{{site_url('accouchement/add_accouchement')}}/?xl={{ femme.id|crypter }}"><i class="fam-table-multiple"></i> ACCOUCHEMENT </a></li>
                                                        <li><a href="{{site_url('accouchement/add_prise__en_charge')}}/?xl={{ femme.id|crypter }}"><i class="fam-table-multiple"></i> PRISE EN CHARGE INITIALE IMMEDIATE</a></li>
                                                        <li><a href="{{site_url('accouchement/add_apgar')}}/?xl={{ femme.id|crypter }}"><i class="fam-table-multiple"></i> APGAR </a></li>
                                                    </ul>    
                                                </div>
                                            </div>

                                        </div>     
                                    </div>   
                                </div>
                            </div>
                            <div class="tab-pane" id="tab2">                               
                                <div id="outcomeCreate">

                                </div>
                            </div>
                            <div class="tab-pane" id="tab3" >           
                                <div id="outputCreate">

                                </div>
                            </div>
                            <div class="form-actions align-right">
                                {#<a href="{{site_url("configuration/resultFramework/reset")}}"  <button type="reset" class="btn"><i class="fam-arrow-rotate-anticlockwise"></i>Reset</button> </a>
                                 {% if operation == 1%} <button style="display: none;" class="btn btn-danger" id="cancel"><i class="fam-cancel"></i>Cancel</button> {%endif%}#}
                                <a href="{{site_url("home/home")}}" <button type="" class="btn btn-inverse"> <i class="ico-arrow-left"></i>{{'Exit'}}</button>  </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>    
{% endblock %}
